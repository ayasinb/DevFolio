@model List<DevFolio.Models.TblAdress>

@foreach (var item in Model)
{
<div class="more-info">
    <p class="lead">
        @item.AdressDescription
    </p>
    <ul class="list-ico">
        <li><span class="ion-ios-location"></span> @item.AdressLocation</li>
        <li><span class="ion-ios-telephone"></span> @item.AdressPhoneNumber</li>
        <li><span class="ion-email"></span> @item.AdressEmail</li>
    </ul>
</div>}
